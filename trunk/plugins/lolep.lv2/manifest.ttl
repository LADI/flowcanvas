@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix foaf:  <http://xmlns.com/foaf/0.1/> .
@prefix lolep: <http://drobilla.net/software/plugins/lolep.lv2/> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix pg:    <http://lv2plug.in/ns/ext/port-groups#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .

lolep:rdf-read a lv2:Plugin ;
    rdfs:seeAlso <rdf-read.ttl> ;
    lv2:binary <rdf-read.so> .

lolep:rdf-write a lv2:Plugin ;
    rdfs:seeAlso <rdf-write.ttl> ;
    lv2:binary <rdf-write.so> .

lolep:object a lv2:Plugin ;
    rdfs:seeAlso <object.ttl> ;
    lv2:binary <object.so> .

lolep:print a lv2:Plugin ;
    rdfs:seeAlso <print.ttl> ;
    lv2:binary <print.so> .

lolep:sum        a lv2:Plugin ; rdfs:seeAlso <sum.ttl>        ; lv2:binary <arith.so> .
lolep:difference a lv2:Plugin ; rdfs:seeAlso <difference.ttl> ; lv2:binary <arith.so> .
lolep:product    a lv2:Plugin ; rdfs:seeAlso <product.ttl>    ; lv2:binary <arith.so> .

lolep:MathPlugin a rdfs:Class ;
    rdfs:subClassOf lv2:Plugin ;
    rdfs:label "Math Operator" .

lolep:TextPlugin a rdfs:Class ;
    rdfs:subClassOf lv2:Plugin ;
    rdfs:label "Text Processor" .

lolep:DataStructurePlugin a rdfs:Class ;
    rdfs:subClassOf lv2:Plugin ;
    rdfs:label "Data Structure" .

